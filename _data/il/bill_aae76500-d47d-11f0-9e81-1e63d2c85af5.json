{"legislative_session": "104th", "identifier": "HB4185", "title": "INSIGNIA AND UNIFORM SALES", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Creates the Official Insignia and Uniform Sales Act. Provides that a person shall not: (1) knowingly manufacture, sell, market, promote, advertise, or otherwise distribute any counterfeit official insignia or uniform; or (2) knowingly manufacture, sell, market, promote, advertise, or otherwise distribute any counterfeit official insignia or uniform to an individual, knowing that the individual is not authorized to possess it under the law of the place in which the insignia or uniform is the official insignia or uniform. Sets forth penalties and defenses for violations of the Act. Defines terms."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Filed with the Clerk by Rep. Barbara Hernandez", "date": "2025-10-28", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["filing"], "related_entities": []}, {"description": "First Reading", "date": "2025-10-28", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["reading-1"], "related_entities": []}, {"description": "Referred to Rules Committee", "date": "2025-10-28", "organization_id": "~{\"classification\": \"lower\"}", "classification": [], "related_entities": []}], "sponsorships": [{"name": "Barbara Hernandez", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"chamber\": \"lower\", \"name\": \"Barbara Hernandez\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "Introduced", "links": [{"url": "https://ilga.gov/documents/legislation/104/HB/PDF/10400HB4185.pdf", "media_type": "application/pdf"}, {"url": "https://ilga.gov/Legislation/BillStatus/FullText?LegDocId=205796&DocName=10400HB4185&DocNum=4185&DocTypeID=HB&LegID=164496&GAID=18&SessionID=114&SpecSess=&S