{"legislative_session": "104th", "identifier": "SB2737", "title": "CONSUMER FRAUD-GROCERY COUPONS", "from_organization": "~{\"classification\": \"upper\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Amends the Consumer Fraud and Deceptive Business Practices Act. Provides that a grocery store that offers a digital coupon to consumers shall make available a corresponding paper coupon of equal value. Provides that the paper coupons shall be easily accessible at the service desk and may also be placed in other locations around the store. Provides that a grocery store that violates the requirement commits an unlawful practice within the meaning of the Act."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Filed with Secretary by Sen. Willie Preston", "date": "2025-10-29", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["filing"], "related_entities": []}, {"description": "First Reading", "date": "2025-10-29", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["reading-1"], "related_entities": []}, {"description": "Referred to Assignments", "date": "2025-10-29", "organization_id": "~{\"classification\": \"upper\"}", "classification": [], "related_entities": []}], "sponsorships": [{"name": "Willie Preston", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"chamber\": \"upper\", \"name\": \"Willie Preston\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "Introduced", "links": [{"url": "https://ilga.gov/documents/legislation/104/SB/PDF/10400SB2737.pdf", "media_type": "application/pdf"}, {"url": "https://ilga.gov/Legislation/BillStatus/FullText?LegDocId=205867&DocName=10400SB2737&DocNum=2737&DocTypeID=SB&LegID=164540&GAID=18&SessionID=114&SpecSess=&Session=&Print=1", "media_type": "text/html"}], "date": "", "classification": ""}], "documents": [], "citations": [], "sources": [{"url": "https://ilga